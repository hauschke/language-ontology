    <owl:AnnotationProperty rdf:about="http://purl.obolibrary.org/obo/LANG_0000003">
        <obo:IAO_0000115 xml:lang="en">The Part 2L Code is a three letter identifier for the language in Part 2L of ISO 639</obo:IAO_0000115>
        <obo:IAO_0000600 xml:lang="en">Eg: The 2L code for english is &apos;eng&apos;</obo:IAO_0000600>
