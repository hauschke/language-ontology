    <owl:AnnotationProperty rdf:about="http://purl.obolibrary.org/obo/LANG_0000002">
        <obo:IAO_0000115 xml:lang="en">The Part 1 code is a unique two letter code used to identify the language.</obo:IAO_0000115>
